import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class LinguagemTest {

	
	@Test
	void test01() {
		Linguagem objeto = new Linguagem();
		boolean retorno = objeto.VerificarPalavra("aabbcc");
		assertEquals(true, retorno);
	}
	@Test
	void test02() {
		Linguagem objeto = new Linguagem();
		boolean retorno = objeto.VerificarPalavra("aaabbbccc");
		assertEquals(true, retorno);
	}
	@Test
	void test03() {
		Linguagem objeto = new Linguagem();
		boolean retorno = objeto.VerificarPalavra("abcabc");
		assertEquals(false, retorno);
	}
	@Test
	void test04() {
		Linguagem objeto = new Linguagem();
		boolean retorno = objeto.VerificarPalavra("aaccbb");
		assertEquals(false, retorno);
	}

}
